{
  "version": 8,
  "name": "simple",
  "sources": {
    "Example": {"type": "vector", "url": "https://example.com/source.json"}
  },
  "sprite": "https://example.com/sprites/sprite",
  "glyphs": "https://example.com/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "road",
      "type": "line",
      "source": "Example",
      "source-layer": "roads",
      "minzoom": 16,
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {"line-width": 5, "line-color": "black"}
    },
    {
      "id": "water",
      "type": "fill",
      "source": "Example",
      "source-layer": "water",
      "minzoom": 16,
      "paint": {"fill-color": "blue"}
    }
  ],
  "id": "example-simple"
}